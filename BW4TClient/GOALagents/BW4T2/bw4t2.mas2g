environment{
	env = "../../environments/BW4TClient.jar" .
	% insert proper initialization parameters and values on the next line, or remove it.
	init = [conf= "blabla.conf", map = "SuperRandom", clientip ="localhost", clientport="2000", serverip = "localhost", serverport = "8000", agentcount = "8", humancount = "1", launchgui = "false", goal = "true"] .
}

agentfiles{
	% insert (list of) agent file references below.
	"demorobot.goal" [name = demorobot] .
	"humanbot.goal" [name = humanbot] .
}

launchpolicy{
	% insert appropriate launch rules for your environment and agents below.
	 when [type=human]@env do launch * :humanbot . 
	 when [type=bot]@env do launch * :demorobot .
	%when [max=3]@env do launch * : demorobot .
}

 
